{
  "name": "figma-link-slides",
  "version": "0.0.0",
  "description": "",
  "keywords": [
    "create-figma-plugin",
    "figma",
    "figma-plugin",
    "figma-plugins"
  ],
  "license": "MIT",
  "author": "Yuan Qing Lim",
  "repository": {
    "type": "git",
    "url": "git://github.com/yuanqing/figma-plugins.git",
    "directory": "packages/figma-link-slides"
  },
  "type": "module",
  "engines": {
    "node": ">=20"
  },
  "files": [
    "src"
  ],
  "scripts": {
    "prebuild": "npm run clean",
    "build": "build-figma-plugin --typecheck --minify",
    "clean": "rimraf '*.log' build 'src/**/*.css.d.ts'",
    "fix": "concurrently npm:fix:css npm:fix:js npm:fix:json",
    "fix:css": "stylelint --fix 'src/**/*.css'",
    "fix:js": "eslint --fix 'src/**/*.{ts,tsx}'",
    "fix:json": "prettier --log-level error --write '*.json'",
    "lint": "concurrently npm:lint:css npm:lint:js",
    "lint:css": "stylelint 'src/**/*.css'",
    "lint:js": "eslint 'src/**/*.{ts,tsx}'",
    "prewatch": "npm run clean",
    "watch": "build-figma-plugin --typecheck --watch"
  },
  "dependencies": {
    "@create-figma-plugin/utilities": "4.0.1"
  },
  "figma-plugin": {
    "name": "Link Slides",
    "id": "1183412674995960577",
    "editorType": [
      "figma"
    ],
    "menu": [
      {
        "name": "Link Slides",
        "main": "src/link-slides/main.ts"
      },
      {
        "name": "Unlink Slides",
        "main": "src/unlink-slides/main.ts"
      },
      "-",
      {
        "name": "Settings",
        "main": "src/settings/main.ts",
        "ui": "src/settings/ui.ts"
      }
    ],
    "relaunchButtons": {
      "link-slides": {
        "name": "Link Slides",
        "main": "src/link-slides/main.ts"
      },
      "unlink-slides": {
        "name": "Unlink Slides",
        "main": "src/unlink-slides/main.ts"
      }
    }
  }
}
